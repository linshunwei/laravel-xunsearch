{
    "name": "linshunwei/laravel-xunsearch",
    "description": "中文全文搜索 xunsearch",
    "keywords": [
        "xunsearch", "XunSearch Engine", "Laravel Scout engine", "Laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "linshunwei",
            "email": "lsw.nba@163.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/scout": "^7.0|^8.0",
        "hightman/xunsearch": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Linshunwei\\XunSearchLaravel\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "vlucas/phpdotenv": "^2.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Linshunwei\\XunSearchLaravel\\XunSearchServiceProvider"
            ]
        }
    }
}
